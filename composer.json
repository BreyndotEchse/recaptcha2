{
    "name": "breyndotechse/recaptcha2",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "captcha",
        "recaptcha",
        "nocaptcha"
    ],
    "homepage": "https://github.com/breyndotechse/recaptcha2",
    "autoload": {
        "psr-4": {
            "ReCaptcha2\\": "src/"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-captcha": "^2.2.3",
